
<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->


    <!-- Main Site -->
    <main id="site-main">
        <div class="container">
            <div class="box-nav d-flex justify-between">
                <a href="/add-user" class="border-shadow">
                    <span class="text-gradient">New User <i class="fas fa-user"></i></span>
                </a>
            </div>

            <!-- form handling -->
            <form action="/Dashboard" method="POST">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                            <th>@Email</th>
                            <th>Password</th>
                            <th>Created at</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%- include('include/_show') %>
                    </tbody>
                </table>
            </form>
        </div>
        <div class="container">
          <div class="box-nav d-flex justify-between">
              <a href="/add-blog" class="border-shadow">
                  <span class="text-gradient">New Blog <i class="fas fa-user"></i></span>
              </a>
          </div>

          <!-- form handling -->
          <form action="/Dashboard" method="POST">
              <table class="table">
                  <thead class="thead-dark">
                      <tr>
                          <th>ID</th>
                          <th>Topic</th>
                          <th>Title</th>
                          <th>Content</th>
                          <th>Status</th>
                          <th>Action</th>
                      </tr>
                  </thead>
                  <tbody>
                      <%- include('include/_showblog') %>
                  </tbody>
              </table>
          </form>
        </div>
        <div class="container">
          <div class="box-nav d-flex justify-between">
              <a href="/" class="border-shadow">
                  <span class="text-gradient">New message <i class="fas fa-user"></i></span>
              </a>
          </div>

          <!-- form handling -->
          <form action="/Dashboard" method="POST">
              <table class="table">
                  <thead class="thead-dark">
                      <tr>
                          <th>ID</th>
                          <th>Name</th>
                          <th>Email</th>
                          <th>Content</th>
                          <th>Status</th>
                          <th>Action</th>
                      </tr>
                  </thead>
                  <tbody>
                      <%- include('include/_showmessage') %>
                  </tbody>
              </table>
          </form>
        </div>
    </main>
<!-- /Main Site -->


<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->